{
  "name": "react16-babel7-webpack4-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "run-p start:client start:server --print-label",
    "start:client": "node ./bin/client-server.js",
    "fmt": "prettier --write src/**/*.js",
    "lint": "eslint src/**/*.js",
    "start:server": "DEBUG=express:*,app:* run-p watch:server lunch:server",
    "lunch:server": "pm2 start server.json --env development --watch --no-daemon -f",
    "watch:server": "webpack --progress --watch --config ./src/webpack/webpack.config.server.dev.js"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.46",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-beta.46",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.0-beta.2",
    "connect-history-api-fallback": "^1.5.0",
    "dotenv-webpack": "^1.5.5",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-lodash-fp": "^2.1.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "html-webpack-plugin": "^3.2.0",
    "koa-connect": "^2.0.1",
    "npm-run-all": "^4.1.3",
    "pm2": "^2.10.4",
    "prettier": "^1.12.1",
    "webpack": "^4.7.0",
    "webpack-cli": "^2.1.2",
    "webpack-node-externals": "^1.7.2",
    "webpack-serve": "^0.3.2"
  },
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.1.2",
    "styled-components": "^3.2.6",
    "styled-reset": "^1.3.4"
  }
}
